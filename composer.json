{
    "name": "grozwalker/spa",
    "authors": [
        {
            "name": "Andrey Groza",
            "email": "andrey_groza@mail.ru"
        }
    ],
    "description": "Test framework",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "phpunit/php-invoker": "^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
